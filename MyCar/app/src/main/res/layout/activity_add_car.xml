<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".SignInActivity">

    <View
        android:id="@+id/country_box7"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="30dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carInfo" />


    <View
        android:id="@+id/country_box6"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/country_box7" />

    <View
        android:id="@+id/country_box4"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/country_box3" />

    <View
        android:id="@+id/country_box3"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/country_box2" />

    <View
        android:id="@+id/country_box2"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/country_box6" />

    <TextView
        android:id="@+id/carInfo"
        android:layout_width="198dp"
        android:layout_height="30dp"
        android:layout_marginTop="56dp"
        android:text="Enter car information"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/country"
        android:layout_width="231dp"
        android:layout_height="54dp"
        android:background="@null"
        android:hint="Model"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/country_box6"
        app:layout_constraintEnd_toEndOf="@+id/country_box6"
        app:layout_constraintHorizontal_bias="0.836"
        app:layout_constraintStart_toStartOf="@+id/country_box6" />

    <EditText
        android:id="@+id/brand"
        android:layout_width="231dp"
        android:layout_height="54dp"
        android:background="@null"
        android:hint="Brand"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/country_box7"
        app:layout_constraintEnd_toEndOf="@+id/country_box7"
        app:layout_constraintHorizontal_bias="0.836"
        app:layout_constraintStart_toStartOf="@+id/country_box7"
        app:layout_constraintTop_toBottomOf="@+id/carInfo"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/email_address"
        android:layout_width="230dp"
        android:layout_height="53dp"
        android:background="@null"
        android:hint="Color"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/country_box2"
        app:layout_constraintEnd_toEndOf="@+id/country_box2"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@+id/country_box2"
        app:layout_constraintTop_toTopOf="@+id/country_box2"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/password"
        android:layout_width="228dp"
        android:layout_height="51dp"
        android:background="@null"
        android:hint="Plate"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/country_box3"
        app:layout_constraintEnd_toEndOf="@+id/country_box3"
        app:layout_constraintHorizontal_bias="0.816"
        app:layout_constraintStart_toStartOf="@+id/country_box3"
        app:layout_constraintTop_toTopOf="@+id/country_box3"
        app:layout_constraintVertical_bias="1.0" />


    <EditText
        android:id="@+id/confirm_your_password"
        android:layout_width="228dp"
        android:layout_height="53dp"
        android:background="@null"
        android:hint="Image"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/country_box4"
        app:layout_constraintEnd_toEndOf="@+id/country_box4"
        app:layout_constraintHorizontal_bias="0.826"
        app:layout_constraintStart_toStartOf="@+id/country_box4"
        app:layout_constraintTop_toTopOf="@+id/country_box4"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/brand_vector"
        android:layout_width="25dp"
        android:layout_height="21dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="136dp"
        android:contentDescription="brand_vector"
        android:src="@drawable/brand_vector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/model_vector"
        android:layout_width="25dp"
        android:layout_height="21dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="40dp"
        android:contentDescription="model_vector"
        android:src="@drawable/model_vector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/brand_vector" />

    <ImageView
        android:id="@+id/color_vector"
        android:layout_width="25dp"
        android:layout_height="21dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="40dp"
        android:contentDescription="color_vector"
        android:src="@drawable/color_vector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/model_vector" />

    <ImageView
        android:id="@+id/plate_vector"
        android:layout_width="31dp"
        android:layout_height="24dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="44dp"
        android:contentDescription="plate_vector"
        android:src="@drawable/plate_vector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/color_vector" />

    <ImageView
        android:id="@+id/image_vector"
        android:layout_width="25dp"
        android:layout_height="21dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="36dp"
        android:contentDescription="image_vector"
        android:src="@drawable/image_vector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/plate_vector" />

    <ImageView
        android:id="@+id/sign_up_button"
        android:layout_width="243dp"
        android:layout_height="35dp"
        android:layout_marginTop="36dp"
        android:contentDescription="sign_up_button"
        android:src="@drawable/sign_in_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/country_box4" />

    <TextView
        android:id="@+id/sign_up"
        android:layout_width="76dp"
        android:layout_height="29dp"
        android:text="Add car"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/sign_up_button"
        app:layout_constraintEnd_toEndOf="@+id/sign_up_button"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="@+id/sign_up_button"
        app:layout_constraintTop_toTopOf="@+id/sign_up_button"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/gradient"
        android:layout_width="wrap_content"
        android:layout_height="129dp"
        android:contentDescription="gradient"
        android:src="@drawable/gradient"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>



</androidx.constraintlayout.widget.ConstraintLayout>