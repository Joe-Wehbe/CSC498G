<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".MoreInfoActivity">

    <ImageView
        android:id="@+id/__img___breaks_group"
        android:layout_width="247dp"
        android:layout_height="143dp"
        android:layout_marginTop="8dp"
        android:contentDescription="__img___breaks_group"
        android:src="@drawable/__img___breaks_group"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/__img___transmission_group" />

    <ImageView
        android:id="@+id/__img___power_steering_group"
        android:layout_width="167dp"
        android:layout_height="200dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="25dp"
        android:contentDescription="__img___power_steering_group"
        android:src="@drawable/__img___power_steering_group"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/__img___engine_coolant_group" />

    <ImageView
        android:id="@+id/__img___transmission_group"
        android:layout_width="167dp"
        android:layout_height="200dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="8dp"
        android:contentDescription="__img___transmission_group"
        android:src="@drawable/__img___transmission_group"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/__img___engine_oil" />

    <ImageView
        android:id="@+id/__img___engine_coolant_group"
        android:layout_width="167dp"
        android:layout_height="200dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="25dp"
        android:contentDescription="__img___engine_coolant_group"
        android:src="@drawable/__img___engine_coolant_group"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/__img___engine_oil"
        android:layout_width="167dp"
        android:layout_height="200dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="36dp"
        android:contentDescription="__img___engine_oil"
        android:src="@drawable/__img___engine_oil"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/gradient"
        android:layout_width="wrap_content"
        android:layout_height="129dp"
        android:contentDescription="gradient"
        android:src="@drawable/gradient"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>