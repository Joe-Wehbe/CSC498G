<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".SignUpActivity">

    <Button
        android:id="@+id/button12"
        android:layout_width="239dp"
        android:layout_height="48dp"
        android:layout_marginTop="36dp"
        android:backgroundTint="@color/dark_gray"
        android:onClick="signUp"
        android:text="Sign up"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/country_box4" />

    <View
        android:id="@+id/country_box4"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/country_box3" />

    <View
        android:id="@+id/country_box3"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/country_box2" />

    <View
        android:id="@+id/country_box2"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/country_box" />

    <ImageView
        android:id="@+id/email_box"
        android:layout_width="138dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="30dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/create_an_account" />

    <ImageView
        android:id="@+id/person_vector"
        android:layout_width="25dp"
        android:layout_height="21dp"
        android:layout_marginTop="136dp"
        android:contentDescription="person_vector"
        android:src="@drawable/person_vector"
        app:layout_constraintEnd_toStartOf="@+id/first_name"
        app:layout_constraintHorizontal_bias="0.875"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/first_name"
        android:layout_width="97dp"
        android:layout_height="48dp"
        android:hint="First Name"
        android:background="@null"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/email_box"
        app:layout_constraintEnd_toEndOf="@+id/email_box"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/email_box"
        app:layout_constraintTop_toTopOf="@+id/email_box"
        app:layout_constraintVertical_bias="0.285" />

    <ImageView
        android:id="@+id/last_name_box"
        android:layout_width="138dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/box"
        app:layout_constraintStart_toEndOf="@+id/email_box"
        app:layout_constraintTop_toBottomOf="@+id/create_an_account" />

    <EditText
        android:id="@+id/last_name"
        android:layout_width="97dp"
        android:layout_height="48dp"
        android:hint="Last Name"
        android:background="@null"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/last_name_box"
        app:layout_constraintEnd_toEndOf="@+id/last_name_box"
        app:layout_constraintHorizontal_bias="0.682"
        app:layout_constraintStart_toStartOf="@+id/last_name_box"
        app:layout_constraintTop_toTopOf="@+id/last_name_box"
        app:layout_constraintVertical_bias="0.571" />

    <TextView
        android:id="@+id/create_an_account"
        android:layout_width="172dp"
        android:layout_height="30dp"
        android:layout_marginTop="56dp"
        android:text="Create an account"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/country_box"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/last_name_box" />

    <EditText
        android:id="@+id/country"
        android:layout_width="231dp"
        android:layout_height="54dp"
        android:hint="Country"
        android:background="@null"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/country_box"
        app:layout_constraintEnd_toEndOf="@+id/country_box"
        app:layout_constraintHorizontal_bias="0.836"
        app:layout_constraintStart_toStartOf="@+id/country_box"
        app:layout_constraintTop_toTopOf="@+id/country_box"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/world_vector"
        android:layout_width="35dp"
        android:layout_height="28dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="36dp"
        android:contentDescription="world_vector"
        android:src="@drawable/world_vector"
        app:layout_constraintStart_toStartOf="@+id/country_box"
        app:layout_constraintTop_toBottomOf="@+id/person_vector" />

    <EditText
        android:id="@+id/email_address1"
        android:layout_width="230dp"
        android:layout_height="53dp"
        android:background="@null"
        android:hint="Email Address"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/country_box2"
        app:layout_constraintEnd_toEndOf="@+id/country_box2"
        app:layout_constraintHorizontal_bias="0.835"
        app:layout_constraintStart_toStartOf="@+id/country_box2"
        app:layout_constraintTop_toTopOf="@+id/country_box2"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/email_vector"
        android:layout_width="28dp"
        android:layout_height="21dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="40dp"
        android:contentDescription="email_vector"
        android:src="@drawable/email_vector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/world_vector" />

    <ImageView
        android:id="@+id/password_vector2"
        android:layout_width="22dp"
        android:layout_height="21dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="44dp"
        android:contentDescription="password_vector2"
        android:src="@drawable/password_vector2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_vector" />

    <ImageView
        android:id="@+id/password_vector1"
        android:layout_width="5dp"
        android:layout_height="5dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="4dp"
        android:contentDescription="password_vector1"
        android:src="@drawable/password_vector1"
        app:layout_constraintBottom_toBottomOf="@+id/password_vector2"
        app:layout_constraintEnd_toEndOf="@+id/password_vector2" />

    <EditText
        android:id="@+id/password"
        android:layout_width="228dp"
        android:layout_height="51dp"
        android:background="@null"
        android:hint="Password"
        android:inputType="textPassword"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/country_box3"
        app:layout_constraintEnd_toEndOf="@+id/country_box3"
        app:layout_constraintHorizontal_bias="0.816"
        app:layout_constraintStart_toStartOf="@+id/country_box3"
        app:layout_constraintTop_toTopOf="@+id/country_box3"
        app:layout_constraintVertical_bias="1.0" />


    <ImageView
        android:id="@+id/check_vector"
        android:layout_width="22dp"
        android:layout_height="20dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="40dp"
        android:contentDescription="check_vector"
        android:src="@drawable/check_vector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/password_vector2" />

    <EditText
        android:id="@+id/confirm_your_password"
        android:layout_width="228dp"
        android:layout_height="53dp"
        android:background="@null"
        android:hint="Confirm your password"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/country_box4"
        app:layout_constraintEnd_toEndOf="@+id/country_box4"
        app:layout_constraintHorizontal_bias="0.826"
        app:layout_constraintStart_toStartOf="@+id/country_box4"
        app:layout_constraintTop_toTopOf="@+id/country_box4"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/gradient"
        android:layout_width="wrap_content"
        android:layout_height="129dp"
        android:contentDescription="gradient"
        android:src="@drawable/gradient"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>



</androidx.constraintlayout.widget.ConstraintLayout>