<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_gray"
    tools:context=".MoreInfoActivity">

    <ImageView
        android:id="@+id/__img___engine_oil"
        android:layout_width="167dp"
        android:layout_height="200dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="84dp"
        android:contentDescription="__img___engine_oil"
        android:src="@drawable/__img___engine_oil"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/__img___breaks_group"
        android:layout_width="247dp"
        android:layout_height="143dp"
        android:layout_marginTop="476dp"
        android:contentDescription="__img___breaks_group"
        android:src="@drawable/__img___breaks_group"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/__img___power_steering_group"
        android:layout_width="167dp"
        android:layout_height="200dp"
        android:layout_marginTop="276dp"
        android:layout_marginEnd="32dp"
        android:contentDescription="__img___power_steering_group"
        android:src="@drawable/__img___power_steering_group"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/__img___engine_coolant_group"
        android:layout_width="167dp"
        android:layout_height="200dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="32dp"
        android:contentDescription="__img___engine_coolant_group"
        android:src="@drawable/__img___engine_coolant_group"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/__img___transmission_group"
        android:layout_width="167dp"
        android:layout_height="200dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="276dp"
        android:contentDescription="__img___transmission_group"
        android:src="@drawable/__img___transmission_group"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sign_into_your_account2"
        android:layout_width="201dp"
        android:layout_height="38dp"
        android:layout_marginTop="24dp"
        android:text="Information on Fluids"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>